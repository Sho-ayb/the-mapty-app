{
  "env": {
    "es6": true,
    "browser": true,
    "es2022": true
  },
  "extends": ["airbnb-base", "prettier", "eslint-config-prettier"],
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": ["error", "unix"],
    "no-unused-vars": "off",
    "no-console": "off",
    "func-names": "off",
    "no-debugger": "off",
    "prettier/prettier": [
      "error",
      { "singleQuote": true, "trailingComma": "es5" }
    ]
  },
  "globals": {
    "L": "readonly"
  },
  "plugins": ["prettier"]
}
